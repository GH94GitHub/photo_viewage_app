pipeline {
  agent any
  stages {
    stage("Clean Up") {
      steps {
        deleteDir()
      }
    }
    stage("Clone") {
      steps {
        sh "git clone -v git@github.com:GH94GitHub/photo_viewage_app.git"
      }
    }
    stage("Clean") {
      steps {
        dir("photo_viewage_app") {
        	sh "chmod u+x ./mvnw"
          sh "./mvnw clean"
        }
      }
    }
    stage("Package") {
      steps {
        dir("photo_viewage_app") {
          sh "./mvnw install"
        }
      }
    }
    stage("Deploy") {
       steps {
         dir("photo_viewage_app/target") {
         	sh "pwd"
           echo "done"
         }
       }
     }
  }
}